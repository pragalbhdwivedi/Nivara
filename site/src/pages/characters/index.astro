---
import MarkdownPage from "../../layouts/MarkdownPage.astro";
import { getCanonCharacters, getCanonCharactersIndexPath, renderCanonMarkdown } from "../../utils/canon.js";

const { routeMap } = await getCanonCharacters();
const indexPath = getCanonCharactersIndexPath();
const { html, toc } = await renderCanonMarkdown(indexPath, routeMap);
---

<MarkdownPage title="Nivara | Characters" heading="Characters" sections={[{ html }]} toc={toc} />
